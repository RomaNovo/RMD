									-let url = "https://rademade.com/"
									a.logo__link(href= url)
										img.logo__img(src="images/logo.svg" alt="logo")
								a.aside__title(href= url) CRM
							.aside__nav.nav
								- var items= [{title:'Application', sublevel: 'gogo'}, {title:'Documentation'}, {title:'Settings'} , 	{title:'Support'}, {title:'Log out'}]
								+list(items)
